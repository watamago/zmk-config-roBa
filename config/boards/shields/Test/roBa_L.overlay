#include "roBa.dtsi"

&kscan0 {
        col-gpios
            = <&xiao_d 10 GPIO_ACTIVE_HIGH>
            , <&xiao_d 9 GPIO_ACTIVE_HIGH> 
            , <&xiao_d 8 GPIO_ACTIVE_HIGH> 
            , <&xiao_d 7 GPIO_ACTIVE_HIGH> 
            , <&gpio0 10 GPIO_ACTIVE_HIGH>
            , <&gpio0 9 GPIO_ACTIVE_HIGH>
            ;
};

&left_encoder {
    status = "okay";
channel@0 {
        reg = <0>;
        zephyr,io-channels = <&adc 0>;
        label = "JOYSTICK_X";
};

channel@1 {
        reg = <1>;
        zephyr,io-channels = <&adc 1>;
        label = "JOYSTICK_Y";
    };
};
